{
  "name": "gh-page-template",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve --config ./webpack.dev.js",
    "format": "prettier --write 'src' 'webpack.*.js'",
    "build": "webpack --config ./webpack.prod.js",
    "generate-image": "node ./generate-image.js",
    "publish": "npm run build && npm run generate-image && node ./publish",
    "pull-template": "git pull https://github.com/maroun-baydoun/gh-page-template.git  master --allow-unrelated-histories"
  },
  "author": {
    "name": "Maroun Baydoun",
    "url": "https://maroun-baydoun.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "babel-loader": "8.2.2",
    "babel-plugin-prismjs": "2.0.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "core-js": "3.8.1",
    "css-loader": "5.0.1",
    "gh-pages": "3.1.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "1.3.5",
    "node-sass": "5.0.0",
    "prettier": "2.2.1",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "node-html-to-image": "3.1.0",
    "prismjs": "1.23.0"
  },
  "features": {
    "react": false,
    "prismjs": false
  }
}
